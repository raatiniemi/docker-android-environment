#!/usr/bin/env bash
render_template () {
  echo '########################';
  echo '# Do not edit this file, it is generated from Dockerfile.template';
  echo '# using the `bin/generate-from-template` script.';
  echo '########################';
  sed -e "s/ %%ANDROID_API%%/${1}/g" "Dockerfile.template";
}

render_template > "Dockerfile";
